<table WIDTH="600px" BORDER="1" CELLPADDING="0" CELLSPACING="0" class="mytitleboxlink">
<!-- BEGIN item -->
 <tr height="25" BGCOLOR="#EEEEEE">
   <td>
   <div align="left">
    <b>
     <font color="#666666">
      {title}
     </font>
    </b>
   </div>
  </td>
  <td width="100" align="right">
     <a href="{href}" title="{title}" class="mytitleboxlink-title">
      {text}
     </a>
  </td>
 </tr>
 <tr>
   <td class="contenido" bgcolor="#FFFFFF" colspan="3" class="mytitleboxlink-content">
    {descripcion}
   </td>
    </tr>
<!-- END item -->
</table>
